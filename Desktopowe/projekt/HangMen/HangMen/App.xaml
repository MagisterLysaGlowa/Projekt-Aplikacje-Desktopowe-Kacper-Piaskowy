<Application x:Class="HangMen.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HangMen"
             xmlns:views="clr-namespace:HangMen.Views"
             xmlns:viewmodels="clr-namespace:HangMen.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:HomeViewModel}">
            <views:Home/>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewmodels:RankingViewModel}">
            <views:Ranking/>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewmodels:PasswordOperationsViewModel}">
            <views:PasswordOperations/>
        </DataTemplate>
              
    </Application.Resources>
</Application>
